{
  "name": "fincamalvasia",
  "version": "1.0.0",
  "description": ".Net Core solution for building the Finca Malvasia Bank front end",
  "main": "index.js",
  "scripts": {
    "assets:dev": "webpack --config webpack.dev.js",
    "assets:prod": "webpack --config webpack.prod.js",
    "build": "SET NODE_ENV=production && npm run assets:prod && dotnet publish -c Release",
    "start": "SET NODE_ENV=development && dotnet watch run",
    "lint": "eslint . --ext .js --cache",
    "stylelint": "stylelint '**/*.scss' --config .stylelintrc.json",
    "validate": "npm ls",
    "svg": "svgo -f ./src/images/icons"
  },
  "author": "Andy Rowland",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.2.0",
    "aspnet-webpack": "^3.0.0",
    "autoprefixer": "^9.4.2",
    "avalanche-css": "^1.3.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.1",
    "dotenv": "^6.1.0",
    "eslint": "^5.9.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "precommit-hook": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "stylelint-scss": "^3.4.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "svg-url-loader": "^2.3.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.27.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.4"
  },
  "pre-commit": [],
  "dependencies": {
    "@babel/polyfill": "^7.0.0"
  }
